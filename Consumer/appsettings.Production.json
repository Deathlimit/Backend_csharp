{
  "RabbitMqSettings": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "OrderCreated": {
      "Queue" : "oms.order.created",
      "BatchSize": 100,
      "BatchTimeoutSeconds": 1
    },
    "OrderStatusChanged": {
      "Queue" : "oms.order.status.changed",
      "BatchSize": 100,
      "BatchTimeoutSeconds": 1
    },
    "DeadLetterExchange": "oms.deadletter",
    "DeadLetterQueue": "oms.deadletter.queue",
    "DeadLetterRoutingKey": "#"
  },
  "HttpClient": {
    "Oms": {
      "BaseAddress": "http://lab1try2-service:8080"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Consumer.Clients.OmsClient": "Information"
    }
  }
}
